{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "businesspanel"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../Business",
      "assets": [
        "assets",
        "favicon.ico",
        "images"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "css/materialize.css",
        "css/style.css",
        
        "css/themes/default/easyui.css",
        "css/themes/angular.css",
        "css/themes/icon.css",
        "css/emailplugin/multiple-emails.css"
      ],
      "scripts": [        
        "Scripts/jquery-1.10.2.min.js",
        "Scripts/CareConnect/materialize.js",
        "Scripts/CareConnect/vendor/jscolor.js",
        "Scripts/CareConnect/emailplugin/multiple-emails.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {
      
    }
  }
}
